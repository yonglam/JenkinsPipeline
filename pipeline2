
pipeline {
  properties([parameters([booleanParam(defaultValue: true, description: '', name: 'is_bool'), string(defaultValue: 'Hello', description: '', name: 'is_string', trim: false)])])

  agent any
  stages {
    stage('CheckOut') {
        steps {
          checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/yonglam/SplitDemo.git']]])
      }
    }
    stage('Build') {
      steps {
        sh 'echo Building...'
      }
    }
    stage('Deploy') {
      steps {
        sh 'echo deploy...'
      }
    }
  }
}
